	{
		width = w;
		height = h;
	}
	virtual int s() = 0;

	virtual int p() = 0;

};
class tetragon : public geometry
{
protected:
	int c;
	int d;
public:
	tetragon(int a, int b, int c1, int d1) :geometry(a, b)
	{
		c = c1;
		d = d1;
	}
